   <section id='qr.constants'>
    &reftitle.constants;
    <table>
     <title>qr constants</title>
      <tgroup cols='3'>
       <thead>
        <row>
         <entry>name</entry>
         <entry>type</entry>
         <entry>descrpition</entry>
        </row>
       </thead>
      <tbody>

        <row>
         <entry>
          <constant id='constantqr-em-auto'>QR_EM_AUTO</constant>
         </entry>
         <entry>int</entry>
         <entry>encoding modeauto
</entry>
        </row>


        <row>
         <entry>
          <constant id='constantqr-em-numeric'>QR_EM_NUMERIC</constant>
         </entry>
         <entry>int</entry>
         <entry>encoding modenumeric
</entry>
        </row>


        <row>
         <entry>
          <constant id='constantqr-em-alnum'>QR_EM_ALNUM</constant>
         </entry>
         <entry>int</entry>
         <entry>encoding modeuppercase alphabet and numeric
</entry>
        </row>


        <row>
         <entry>
          <constant id='constantqr-em-8bit'>QR_EM_8BIT</constant>
         </entry>
         <entry>int</entry>
         <entry>encoding mode8-bit byte
</entry>
        </row>


        <row>
         <entry>
          <constant id='constantqr-em-kanji'>QR_EM_KANJI</constant>
         </entry>
         <entry>int</entry>
         <entry>encoding modeJIS X 0208 Kanji
</entry>
        </row>


        <row>
         <entry>
          <constant id='constantqr-ecl-l'>QR_ECL_L</constant>
         </entry>
         <entry>int</entry>
         <entry>error correction level: L (7%)
</entry>
        </row>


        <row>
         <entry>
          <constant id='constantqr-ecl-m'>QR_ECL_M</constant>
         </entry>
         <entry>int</entry>
         <entry>error correction level: M (15%)
</entry>
        </row>


        <row>
         <entry>
          <constant id='constantqr-ecl-q'>QR_ECL_Q</constant>
         </entry>
         <entry>int</entry>
         <entry>error correction level: Q (25%)
</entry>
        </row>


        <row>
         <entry>
          <constant id='constantqr-ecl-h'>QR_ECL_H</constant>
         </entry>
         <entry>int</entry>
         <entry>error correction level: H (30%)
</entry>
        </row>


        <row>
         <entry>
          <constant id='constantqr-fmt-digit'>QR_FMT_DIGIT</constant>
         </entry>
         <entry>int</entry>
         <entry>output format: DIGIT (space separated array of 0 and 1)
</entry>
        </row>


        <row>
         <entry>
          <constant id='constantqr-fmt-ascii'>QR_FMT_ASCII</constant>
         </entry>
         <entry>int</entry>
         <entry>output format: ASCII ART (white module is " " and black module is "*")
</entry>
        </row>


        <row>
         <entry>
          <constant id='constantqr-fmt-json'>QR_FMT_JSON</constant>
         </entry>
         <entry>int</entry>
         <entry>output format: JSON (JavaScript style multi dimension array of 0 and 1)
</entry>
        </row>


        <row>
         <entry>
          <constant id='constantqr-fmt-pbm'>QR_FMT_PBM</constant>
         </entry>
         <entry>int</entry>
         <entry>output format: PBM (Portable Bitmap)
</entry>
        </row>


        <row>
         <entry>
          <constant id='constantqr-fmt-bmp'>QR_FMT_BMP</constant>
         </entry>
         <entry>int</entry>
         <entry>output format: BMP (Windows Bitmap)
</entry>
        </row>


        <row>
         <entry>
          <constant id='constantqr-fmt-svg'>QR_FMT_SVG</constant>
         </entry>
         <entry>int</entry>
         <entry>output format: SVG (XML/Scalable Vector Graphics)
</entry>
        </row>


        <row>
         <entry>
          <constant id='constantqr-fmt-tiff'>QR_FMT_TIFF</constant>
         </entry>
         <entry>int</entry>
         <entry>output format: TIFF
</entry>
        </row>


        <row>
         <entry>
          <constant id='constantqr-fmt-png'>QR_FMT_PNG</constant>
         </entry>
         <entry>int</entry>
         <entry>output format: PNG
</entry>
        </row>


     </tbody>
    </tgroup>
   </table>
   </section>
   
  </partintro>

&reference.qr.functions;

